{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default capability and permissions for the app.",
  "windows": ["main"],
  "urls": ["local://*"],
  "permissions": [
    "core:default",
    "core:path:default",
    "core:path:allow-resolve-directory",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-message",
    "shell:allow-open",
    "updater:default"
  ]
}
